<section class="services" id="services">
  <div class="container">
    <div class="content">
      <h2>Services That I Give You</h2>
      <div class="ser">
        <% for(var index = 0; index < services.length ; index++) { %>
        <div class="box">
          <h2>0<%= index+1 %></h2>
          <h3><%= services[index].name%></h3>
          <p><%= services[index].description%></p>
        </div>
        <%}%>
      </div>
    </div>
  </div>
  <!-- My work -->
  <div class="info">
    <h2>My Works</h2>
    <div class="boxes">
      <% for(var index = 0; index < mywork.length ; index++) { %>
      <a href="<%= mywork[index].url %> ">
        <img src="images/<%= mywork[index].image%>" alt="" />
        <p>Click</p>
      </a>
      <%}%>
    </div>
  </div>
</section>
